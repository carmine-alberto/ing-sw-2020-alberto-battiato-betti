<gods>

    <god>
        <name>Apollo</name>
        <actionPredicates></actionPredicates>
        <movePredicates>
            <and>
                <movePredicate>
                    <name>IsDeltaHeightLessThanPredicate</name>
                    <args>
                        <arg>1</arg>
                    </args>
                </movePredicate>
                <or>
                    <movePredicate>ContainsOpponentWorkerPredicate</movePredicate>
                    <movePredicate>IsCellFreePredicate</movePredicate>
                </or>
            </and>
        </movePredicates>
        <buildPredicates></buildPredicates>
        <constructiblePredicates></constructiblePredicates>
        <winConditions></winConditions>
        <moveStrategy>MoveWithSwap</moveStrategy>
        <buildStrategy></buildStrategy>
    </god>

    <god>
        <name>Artemis</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phase>MovePhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <phase>MovePhase</phase>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
            </phases>
            <phases>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
            </phases>
        </phases>
        <actionPredicates></actionPredicates>
        <movePredicates>
            <movePredicate>NotInOldMovePositionPredicate</movePredicate>
        </movePredicates>
        <buildPredicates></buildPredicates>
        <constructiblePredicates></constructiblePredicates>
        <winConditions></winConditions>
    </god>

    <god>
        <name>Athena</name>
        <actionPredicates></actionPredicates>
        <movePredicates></movePredicates>
        <buildPredicates></buildPredicates>
        <constructiblePredicates></constructiblePredicates>
        <winConditions></winConditions>
        <moveStrategy>
            <name>MoveAndSet</name>
            <args>
                <arg>CantMoveUpPredicate</arg>
            </args>
        </moveStrategy>
    </god>

    <god>
        <name>Atlas</name>
        <actionPredicates></actionPredicates>
        <movePredicates></movePredicates>
        <buildPredicates></buildPredicates>
        <constructiblePredicates>
            <name>BlockPredicate</name>
            <args>
                <arg>0</arg>
            </args>
        </constructiblePredicates>
        <winConditions></winConditions>
    </god>

    <god>
        <name>Demetra</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phase>MovePhase</phase>
            <phase>BuildPhase</phase>
            <phase>ChooseBlockPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
            </phases>
        </phases>
        <actionPredicates></actionPredicates>
        <movePredicates></movePredicates>
        <buildPredicates>
            <buildPredicate>
                <name>NotInOldBuildPositionPredicate</name>
            </buildPredicate>
        </buildPredicates>
        <constructiblePredicates></constructiblePredicates>
        <winConditions></winConditions>
    </god>

    <god>
        <name>Ephestus</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phase>MovePhase</phase>
            <phase>BuildPhase</phase>
            <phase>ChooseBlockPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
            </phases>
        </phases>
        <actionPredicates></actionPredicates>
        <movePredicates></movePredicates>
        <buildPredicates>
            <not>
                <buildPredicate>
                    <name>NotInOldBuildPositionPredicate</name>
                </buildPredicate>
            </not>
        </buildPredicates>
        <constructiblePredicates></constructiblePredicates>
        <winConditions></winConditions>
    </god>

    <god>
        <name>Minotaur</name>
        <actionPredicates></actionPredicates>
        <movePredicates>
            <and>
                <movePredicate>
                    <name>IsDeltaHeightLessThanPredicate</name>
                    <args>
                        <arg>1</arg>
                    </args>
                </movePredicate>
                <or>
                    <and>
                        <movePredicate>ContainsOpponentWorkerPredicate</movePredicate>
                        <movePredicate>CanMoveBackPredicate</movePredicate>
                    </and>
                    <movePredicate>IsCellFreePredicate</movePredicate>
                </or>
            </and>
        </movePredicates>
        <buildPredicates></buildPredicates>
        <constructiblePredicates></constructiblePredicates>
        <winConditions></winConditions>
        <moveStrategy>MoveAndShiftBack</moveStrategy>
        <buildStrategy></buildStrategy>
    </god>

    <god>
        <name>Pan</name>
        <actionPredicates></actionPredicates>
        <movePredicates></movePredicates>
        <buildPredicates></buildPredicates>
        <constructiblePredicates></constructiblePredicates>
        <winConditions>
            <winCondition>
                <name>HasMovedDownPredicate</name>
                <args>
                    <arg>2</arg>
                </args>
            </winCondition>
        </winConditions>
        <buildStrategy></buildStrategy>
    </god>

    <god>
        <name>Prometheus</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
                <phase>
                    <name>MovePhase</name>
                    <movePredicates>
                        <movePredicate>
                            <name>IsDeltaHeightLessThanPredicate</name>
                            <args>
                                <arg>0</arg>
                            </args>
                        </movePredicate>
                    </movePredicates>
                </phase>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
            </phases>
            <phases>
                <phase>MovePhase</phase>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
            </phases>
        </phases>
        <actionPredicates></actionPredicates>
        <movePredicates></movePredicates>
        <buildPredicates></buildPredicates>
        <constructiblePredicates></constructiblePredicates>
        <winConditions></winConditions>
        <moveStrategy></moveStrategy>
        <buildStrategy></buildStrategy>
    </god>

</gods>