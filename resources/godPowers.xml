<gods>

    <god>
        <name>Apollo</name>
        <movePredicates>
            <and>
                <movePredicate>
                    <name>IsDeltaHeightLessThanPredicate</name>
                    <arg>2</arg>
                </movePredicate>
                <or>
                    <movePredicate>ContainsOpponentWorkerPredicate</movePredicate>
                    <movePredicate>IsCellFreePredicate</movePredicate>
                </or>
            </and>
        </movePredicates>
        <moveStrategy>MoveWithSwap</moveStrategy>
    </god>

    <god>
        <name>Artemis</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>MovePhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <phase>
                    <name>MovePhase</name>
                    <movePredicates>
                        <and>
                            <and>
                                <movePredicate>
                                    <name>IsDeltaHeightLessThanPredicate</name>
                                    <arg>2</arg>
                                </movePredicate>
                                <movePredicate>IsCellFreePredicate</movePredicate>
                            </and>
                            <movePredicate>NotInOldMovePositionPredicate</movePredicate>
                        </and>
                    </movePredicates>
                </phase>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
                <phase>EndPhase</phase>
            </phases>
            <phases>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
                <phase>EndPhase</phase>
            </phases>
        </phases>
    </god>

    <god>
        <name>Athena</name>
        <moveStrategy>
            <name>MoveAndSet</name>
            <arg>
                <movePredicate>
                    <name>IsDeltaHeightLessThanPredicate</name>
                    <arg>1</arg>
                </movePredicate>
            </arg>
        </moveStrategy>
    </god>

    <god>
        <name>Atlas</name>
        <constructiblePredicates>
            <name>BlockPredicate</name>
            <arg>0</arg>
        </constructiblePredicates>
    </god>

    <god>
        <name>Demetra</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>MovePhase</phase>
            <phase>BuildPhase</phase>
            <phase>ChooseBlockPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <phase>
                    <name>BuildPhase</name>
                    <buildPredicates>
                        <and>
                            <buildPredicate>IsCellFreePredicate</buildPredicate>
                            <buildPredicate>NotInOldBuildPositionPredicate</buildPredicate>
                        </and>
                    </buildPredicates>
                </phase>
                <phase>ChooseBlockPhase</phase>
                <phase>EndPhase</phase>
            </phases>
            <phases>
                <phase>EndPhase</phase>
            </phases>
        </phases>
    </god>

    <god>
        <name>Hephaestus</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>MovePhase</phase>
            <phase>BuildPhase</phase>
            <phase>ChooseBlockPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <phase>
                    <name>BuildPhase</name>
                    <buildPredicates>
                        <and>
                            <buildPredicate>
                                <name>MaxHeightPredicate</name>
                                <arg>2</arg>
                            </buildPredicate>
                            <and>
                                <movePredicate>IsCellFreePredicate</movePredicate>
                                <negate>
                                    <buildPredicate>NotInOldBuildPositionPredicate</buildPredicate>
                                </negate>
                            </and>
                        </and>
                    </buildPredicates>
                </phase>
                <phase>ChooseBlockPhase</phase>
                <phase>EndPhase</phase>
            </phases>
            <phases>
                <phase>EndPhase</phase>
            </phases>
        </phases>
    </god>

    <god>
        <name>Minotaur</name>
        <movePredicates>
            <and>
                <movePredicate>
                    <name>IsDeltaHeightLessThanPredicate</name>
                    <arg>2</arg>
                </movePredicate>
                <or>
                    <movePredicate>IsCellFreePredicate</movePredicate>
                    <and>
                        <movePredicate>ContainsOpponentWorkerPredicate</movePredicate>
                        <movePredicate>CanMoveBackPredicate</movePredicate>
                    </and>
                </or>
            </and>
        </movePredicates>
        <moveStrategy>MoveAndShiftBack</moveStrategy>
    </god>

    <god>
        <name>Pan</name>
        <winConditions>
            <or>
                <winCondition>WinningMovePredicate</winCondition>
                <winCondition>
                    <name>HasMovedDownPredicate</name>
                    <arg>2</arg>
                </winCondition>
            </or>
        </winConditions>
    </god>

    <god>
        <name>Prometheus</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
                <phase>
                    <name>MovePhase</name>
                    <movePredicates>
                        <and>
                            <movePredicate>
                                <name>IsDeltaHeightLessThanPredicate</name>
                                <arg>1</arg>
                            </movePredicate>
                            <movePredicate>IsCellFreePredicate</movePredicate>
                        </and>
                    </movePredicates>
                </phase>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
                <phase>EndPhase</phase>
            </phases>
            <phases>
                <phase>MovePhase</phase>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
                <phase>EndPhase</phase>
            </phases>
        </phases>
    </god>

    <god>
        <name>Charon</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <phase>DisplacePhase</phase>
                <phase>MovePhase</phase>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
                <phase>EndPhase</phase>
            </phases>
            <phases>
                <phase>MovePhase</phase>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
                <phase>EndPhase</phase>
            </phases>
        </phases>
    </god>

    <god>
        <name>Chronus</name>
        <winConditions>
            <or>
                <winCondition>WinningMovePredicate</winCondition>
                <winCondition>
                    <name>CompleteTowersPredicate</name>
                    <arg>5</arg>
                </winCondition>
            </or>
        </winConditions>
    </god>

    <god>
        <name>Hera</name>
        <winConditions>
            <outerPredicates>
                <winCondition>NotOnPerimeterPredicate</winCondition>
            </outerPredicates>
        </winConditions>
    </god>

    <god>
        <name>Hestia</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>MovePhase</phase>
            <phase>BuildPhase</phase>
            <phase>ChooseBlockPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <phase>
                    <name>BuildPhase</name>
                    <buildPredicates>
                        <and>
                            <buildPredicate>IsCellFreePredicate</buildPredicate>
                            <buildPredicate>NotOnPerimeterPredicate</buildPredicate>
                        </and>
                    </buildPredicates>
                </phase>
                <phase>ChooseBlockPhase</phase>
                <phase>EndPhase</phase>
            </phases>
            <phases>
                <phase>EndPhase</phase>
            </phases>
        </phases>
    </god>

    <god>
        <name>Zeus</name>
        <buildPredicates>
            <or>
                <buildPredicate>IsCellFreePredicate</buildPredicate>
                <buildPredicate>CanBuildUnderItselfPredicate</buildPredicate>
            </or>
        </buildPredicates>
    </god>

</gods>