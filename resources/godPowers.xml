<gods>
    <god>
        <name>Apollo</name>
        <movePredicates>
            <and>
                <movePredicate>
                    <name>IsDeltaHeightLessThanPredicate</name>
                    <arg>1</arg>
                </movePredicate>
                <or>
                    <movePredicate>ContainsOpponentWorkerPredicate</movePredicate>
                    <movePredicate>IsCellFreePredicate</movePredicate>
                </or>
            </and>
        </movePredicates>
        <moveStrategy>MoveWithSwap</moveStrategy>
    </god>

    <god>
        <name>Artemis</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phase>MovePhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <phase>MovePhase</phase>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
            </phases>
            <phases>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
            </phases>
        </phases>
        <movePredicates>
            <movePredicate>NotInOldMovePositionPredicate</movePredicate>
        </movePredicates>
    </god>

    <god>
        <name>Athena</name>
        <moveStrategy>
            <name>MoveAndSet</name>
            <arg>
                <movepredicates>
                        <negate>
                            <movepredicate>CanMoveUpPredicate</movepredicate>
                        </negate>
                </movepredicates>
            </arg>
        </moveStrategy>
    </god>

    <god>
        <name>Atlas</name>
        <constructiblePredicates>
            <name>BlockPredicate</name>
            <arg>0</arg>
        </constructiblePredicates>
    </god>

    <god>
        <name>Demetra</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phase>MovePhase</phase>
            <phase>BuildPhase</phase>
            <phase>ChooseBlockPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
            </phases>
        </phases>
        <buildPredicates>
            <buildPredicate>NotInOldBuildPositionPredicate</buildPredicate>
        </buildPredicates>
    </god>

    <god>
        <name>Ephestus</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phase>MovePhase</phase>
            <phase>BuildPhase</phase>
            <phase>ChooseBlockPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
            </phases>
        </phases>
        <buildPredicates>
            <negate>
                <buildPredicate>NotInOldBuildPositionPredicate</buildPredicate>
            </negate>
        </buildPredicates>
    </god>

    <god>
        <name>Minotaur</name>
        <movePredicates>
            <and>
                <movePredicate>
                    <name>IsDeltaHeightLessThanPredicate</name>
                    <arg>1</arg>
                </movePredicate>
                <or>
                    <and>
                        <movePredicate>ContainsOpponentWorkerPredicate</movePredicate>
                        <movePredicate>CanMoveBackPredicate</movePredicate>
                    </and>
                    <movePredicate>IsCellFreePredicate</movePredicate>
                </or>
            </and>
        </movePredicates>
        <moveStrategy>MoveAndShiftBack</moveStrategy>
    </god>

    <god>
        <name>Pan</name>
        <winConditions>
            <or>
                <winCondition>
                    <name>HasMovedDownPredicate</name>
                    <arg>2</arg>
                </winCondition>
                <winCondition>WinningMovePredicate</winCondition>
            </or>
        </winConditions>
    </god>

    <god>
        <name>Prometheus</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
                <phase>
                    <name>MovePhase</name>
                    <movePredicates>
                        <movePredicate>
                            <name>IsDeltaHeightLessThanPredicate</name>
                            <arg>0</arg>
                        </movePredicate>
                    </movePredicates>
                </phase>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
            </phases>
            <phases>
                <phase>MovePhase</phase>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
            </phases>
        </phases>
    </god>

    <god>
        <name>Charon</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <!-- <phase>DISPLACE</phase> -->
                <phase>ChooseActionPhase</phase>
                <phase>MovePhase</phase>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
            </phases>
            <phases>
                <phase>MovePhase</phase>
                <phase>BuildPhase</phase>
                <phase>ChooseBlockPhase</phase>
            </phases>
        </phases>
    </god>

    <god>
        <name>Chronus</name>
        <winConditions>
            <or>
                <winCondition>WinningMovePredicate</winCondition>
                <winCondition>
                    <name>CompleteTowersPredicate</name>
                    <arg>5</arg>
                </winCondition>
            </or>
        </winConditions>
    </god>

    <god>
        <name>Hera</name>
        <winConditions>
            <and>
                <winCondition>WinningMovePredicate</winCondition>
                <winCondition>
                    <name>NotOnPerimeterPredicate</name>
                    <application>opponents</application>
                </winCondition>
            </and>
        </winConditions>
    </god>

    <god>
        <name>Hestia</name>
        <phases>
            <phase>ChooseWorkerPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phase>MovePhase</phase>
            <phase>BuildPhase</phase>
            <phase>ChooseBlockPhase</phase>
            <phase>ChooseActionPhase</phase>
            <phases>
                <phase>BuildPhase</phase>
                <buildPredicates>
                    <and>
                        <buildPredicate>IsCellFreePredicate</buildPredicate>
                        <buildPredicate>NotOnPerimeterPredicate></buildPredicate>
                    </and>
                </buildPredicates>
                <phase>ChooseBlockPhase</phase>
            </phases>
        </phases>
    </god>

    <god>
        <name>Zeus</name>
        <buildPredicates>
            <or>
                <buildPredicate>IsCellFreePredicate</buildPredicate>
                <buildPredicate>CanBuildUnderItselfPredicate</buildPredicate>
            </or>
        </buildPredicates>
    </god>
</gods>